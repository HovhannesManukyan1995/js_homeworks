
pipeline {
    agent any

        triggers {
            GenericTrigger(
                genericVariables: [
                    [defaultValue: '', key: 'base', regexpFilter: '', value: '$.ref'],
                    ],
             causeString: 'Triggered on $ref',
             token: 'test',
             tokenCredentialId: '' )
          }
stages{
    stage('check branch name'){
        steps{
            echo 'hello world'
            echo env.base'
        }
    }
}
}
    
